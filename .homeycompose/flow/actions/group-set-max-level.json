{
  "title": {
    "en": "Set group max level",
    "ko": "그룹 최대 밝기 설정"
  },
  "titleFormatted": {
    "en": "Set group [[group]] max level to [[level]] on bus [[bus]]",
    "ko": "버스 [[bus]]의 그룹 [[group]] 최대 밝기를 [[level]]로 설정"
  },
  "args": [
    {
      "type": "device",
      "name": "device",
      "filter": "driver_id=dali-controller"
    },
    {
      "type": "range",
      "name": "bus",
      "title": {
        "en": "Bus",
        "ko": "버스"
      },
      "min": 0,
      "max": 3,
      "step": 1,
      "labelDecimals": 0,
      "placeholder": {
        "en": "Bus (0-3)",
        "ko": "버스 (0-3)"
      }
    },
    {
      "type": "range",
      "name": "group",
      "title": {
        "en": "Group",
        "ko": "그룹"
      },
      "min": 0,
      "max": 15,
      "step": 1,
      "labelDecimals": 0,
      "placeholder": {
        "en": "Group (0-15)",
        "ko": "그룹 (0-15)"
      }
    },
    {
      "type": "range",
      "name": "level",
      "title": {
        "en": "Max Level",
        "ko": "최대 밝기"
      },
      "min": 0,
      "max": 254,
      "step": 1,
      "labelDecimals": 0,
      "placeholder": {
        "en": "Level (0-254)",
        "ko": "밝기 (0-254)"
      }
    }
  ]
}
