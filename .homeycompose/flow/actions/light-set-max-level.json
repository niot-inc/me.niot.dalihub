{
  "title": {
    "en": "Set light max level",
    "ko": "조명 최대 밝기 설정"
  },
  "titleFormatted": {
    "en": "Set light [[address]] max level to [[level]] on bus [[bus]]",
    "ko": "버스 [[bus]]의 조명 [[address]] 최대 밝기를 [[level]]로 설정"
  },
  "args": [
    {
      "type": "device",
      "name": "device",
      "filter": "driver_id=dali-controller"
    },
    {
      "type": "range",
      "name": "bus",
      "title": {
        "en": "Bus",
        "ko": "버스"
      },
      "min": 0,
      "max": 3,
      "step": 1,
      "labelDecimals": 0,
      "placeholder": {
        "en": "Bus (0-3)",
        "ko": "버스 (0-3)"
      }
    },
    {
      "type": "range",
      "name": "address",
      "title": {
        "en": "Address",
        "ko": "주소"
      },
      "min": 0,
      "max": 63,
      "step": 1,
      "labelDecimals": 0,
      "placeholder": {
        "en": "Address (0-63)",
        "ko": "주소 (0-63)"
      }
    },
    {
      "type": "range",
      "name": "level",
      "title": {
        "en": "Max Level",
        "ko": "최대 밝기"
      },
      "min": 0,
      "max": 254,
      "step": 1,
      "labelDecimals": 0,
      "placeholder": {
        "en": "Level (0-254)",
        "ko": "밝기 (0-254)"
      }
    }
  ]
}
