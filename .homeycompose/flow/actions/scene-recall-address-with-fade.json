{
  "title": {
    "en": "Recall scene to address with fade time",
    "ko": "페이드 시간과 함께 주소에 씬 호출"
  },
  "titleFormatted": {
    "en": "Recall scene [[scene]] to address [[address]] on bus [[bus]] with fade time [[fadeTime]]",
    "ko": "버스 [[bus]]의 주소 [[address]]에 씬 [[scene]]을 페이드 시간 [[fadeTime]]과 함께 호출"
  },
  "args": [
    {
      "type": "device",
      "name": "device",
      "filter": "driver_id=dali-controller"
    },
    {
      "type": "range",
      "name": "bus",
      "title": {
        "en": "Bus",
        "ko": "버스"
      },
      "min": 0,
      "max": 3,
      "step": 1,
      "labelDecimals": 0,
      "placeholder": {
        "en": "Bus (0-3)",
        "ko": "버스 (0-3)"
      }
    },
    {
      "type": "range",
      "name": "scene",
      "title": {
        "en": "Scene",
        "ko": "씬"
      },
      "min": 0,
      "max": 15,
      "step": 1,
      "labelDecimals": 0,
      "placeholder": {
        "en": "Scene (0-15)",
        "ko": "씬 (0-15)"
      }
    },
    {
      "type": "range",
      "name": "address",
      "title": {
        "en": "Address",
        "ko": "주소"
      },
      "min": 0,
      "max": 63,
      "step": 1,
      "labelDecimals": 0,
      "placeholder": {
        "en": "Address (0-63)",
        "ko": "주소 (0-63)"
      }
    },
    {
      "type": "dropdown",
      "name": "fadeTime",
      "title": {
        "en": "Fade Time",
        "ko": "페이드 시간"
      },
